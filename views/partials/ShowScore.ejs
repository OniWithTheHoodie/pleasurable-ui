<ul class="showscore">
    <% if (feedback.length > 0) { %>
        <% if (feedback[feedback.length - 1].rating) { %>
            <li>Algemeen : <%= feedback[feedback.length - 1].rating.kitchen %></li>
            <li>Keuken : <%= feedback[feedback.length - 1].rating.general %></li>
            <li>Badkamer: <%= feedback[feedback.length - 1].rating.bathroom %></li>
            <li>Tuin: <%= feedback[feedback.length - 1].rating.garden %></li>
        <% } else { %>
            <p class="">er is geen score ingevoerd</p>
        <% } %>
    <% } else { %>
        <p>er is geen score ingevoerd.</p>
    <% } %>
</ul>



