<ul class="show_notes">
    <% if (feedback.length > 0) { %>
        <% if (feedback[feedback.length - 1].note) { %>
            <li> <%=(feedback[feedback.length - 1].note) %></li>
        <% }


        else { %>
            <p>No note for this house.</p>
        <% } %>
    <% }

    else { %>
        <p>No feedback available.</p>
    <% } %>
</ul>

